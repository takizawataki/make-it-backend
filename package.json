{
  "name": "make-it-backend",
  "version": "0.1.0",
  "bin": {
    "make-it-backend": "bin/make-it-backend.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "JEST_SNAPSHOT_DIR=$(pwd)/test/__snapshots__/development jest test/make-it-backend-development.test.ts",
    "test:staging": "JEST_SNAPSHOT_DIR=$(pwd)/test/__snapshots__/staging jest test/make-it-backend-staging.test.ts",
    "test:production": "JEST_SNAPSHOT_DIR=$(pwd)/test/__snapshots__/production jest test/make-it-backend-production.test.ts",
    "update-snapshot:development": "JEST_SNAPSHOT_DIR=$(pwd)/test/__snapshots__/development jest test/make-it-backend-development.test.ts --updateSnapshot",
    "update-snapshot:staging": "JEST_SNAPSHOT_DIR=$(pwd)/test/__snapshots__/staging jest test/make-it-backend-staging.test.ts --updateSnapshot",
    "update-snapshot:production": "JEST_SNAPSHOT_DIR=$(pwd)/test/__snapshots__/production jest test/make-it-backend-production.test.ts --updateSnapshot",
    "update-snapshot-ci": "npm run update-snapshot:development && npm run update-snapshot:staging && npm run update-snapshot:production",
    "cdk": "cdk",
    "lint": "eslint . --ext ts --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write . --config .prettierrc.json",
    "prepare": "husky install",
    "generate:model": "openapi-generator-cli generate -i ../make-it-api-specification/make-it-api-spec.yaml -g typescript-fetch -o ./lambda-ts/types/generated --global-property models"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.13.5",
    "@types/aws-lambda": "^8.10.145",
    "@types/jest": "^29.5.12",
    "@types/node": "20.14.9",
    "@typescript-eslint/eslint-plugin": "^8.0.1",
    "@typescript-eslint/parser": "^8.0.1",
    "aws-cdk": "^2.151.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.8",
    "prettier": "^3.3.3",
    "ts-jest": "^29.1.5",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "~5.5.3"
  },
  "dependencies": {
    "@aws-cdk/aws-lambda-python-alpha": "^2.151.1-alpha.0",
    "@aws-sdk/client-cognito-identity-provider": "^3.645.0",
    "@aws-sdk/client-dynamodb": "^3.654.0",
    "@aws-sdk/client-firehose": "^3.654.0",
    "@aws-sdk/client-secrets-manager": "^3.658.1",
    "@aws-sdk/client-sesv2": "^3.637.0",
    "@aws-sdk/lib-dynamodb": "^3.637.0",
    "@aws-sdk/util-dynamodb": "^3.654.0",
    "@langchain/aws": "^0.1.0",
    "@langchain/community": "^0.3.3",
    "@langchain/core": "^0.3.3",
    "@langchain/langgraph": "^0.2.11",
    "amazon-cognito-passwordless-auth": "^0.14.0",
    "aws-cdk-lib": "^2.149.0",
    "constructs": "^10.0.0",
    "source-map-support": "^0.5.21"
  }
}
